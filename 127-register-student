public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c apxio             = new apxio__Student__c();
    apxio.apxio__Email__c               = email;
    apxio.apxio__Phone__c               = phone;
    apxio.name                          = name;

    insert apxio;

    apxio__Student__c axpioValue = [Select Id, apxio__Registration_Number__c From apxio__Student__c where Id = :apxio.id Limit 1];

    return axpioValue.apxio_Registration_Number__c;
}


