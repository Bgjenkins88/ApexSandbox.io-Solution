public String ageGroup(Integer n) {
    //code here
    //Integer childAge = n;

    if (n <= 1){

        return 'infant';
    }

    else if ( n >= 2 && n <= 14){

        return 'child';
    }

    else if (n >= 15 && n <= 21){

        return 'youth';
    }

    else ( n >= 22){


        return 'Adult';
    }
}