public String ageGroup(Integer n) {
    //code here
    Integer childAge = n;


    if (childAge <= 1){
        system.debug('made it inside infant: value of child age is ' + childAge);
        return 'infant';
    }

    if ( n >= 2 && n <= 14){

        return 'child';
    }

     if (n >= 15 && n <= 21){

        return 'youth';
    }

    else ( n >= 22){


        return 'Adult';
    }
}