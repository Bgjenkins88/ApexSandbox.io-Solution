public Id insertCourse(String name, String details, Integer credits) {
    // code here
        apxio__Course__c s        = new apxio__Course__c();
        s.axpio_course_details__c = details;
        s.axpio_credits__c        = (String)credits; 
        s.name                    = name;

        insert s;

        return s.Id;


    
}