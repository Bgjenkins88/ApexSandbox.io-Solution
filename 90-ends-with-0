public Boolean isEndWithZero(Integer num){
     //code here
        String stringNumber = string.valueOf(num):

        stringNumber = stringNumber.right(1);

        Integer finalNumber = Integer.valueOf(stringNumber);

        Boolean finalValue;
   
     if (finalNumber == 0){

         finalValue = True;
     }

     if (finalNumber != 0){

         finalValue = False;
     }

     return finalValue;
}