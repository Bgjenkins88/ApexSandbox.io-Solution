public Boolean isEndWithZero(Integer num){
     //code here
        String stringNumber = string.valueOf(num):

        String lastNumber   = stringNumber.right(1);

        //Integer finalNumber = Integer.valueOf(stringNumber);

        Boolean finalValue;
   
     if (lastNumber == '0'){

         finalValue = True;
     }

     else (lastNumber != '0'){

         finalValue = False;
     }

     return finalValue;
}