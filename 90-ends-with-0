public Boolean isEndWithZero(Integer num){
     //code here
        String stringNumber = string.valueOf(num):

        stringNumber        = stringNumber.right(0);

        Integer finalNumber = Integer.valueOf(stringNumber);

        Boolean finalValue;
   
     if (stringNumber == '0'){

         finalValue = True;
     }

     if (stringNumber != '0'){

         finalValue = False;
     }

     return finalValue;
}