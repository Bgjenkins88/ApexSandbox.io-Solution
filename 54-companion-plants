public Boolean companionPlants(String plant1, String plant2) {
    //code here
    if ((plant1 = 'lettuce'&& (plant2 == 'cucumber'|| plant2 =='onions')) || (plant2 = 'lettuce' &&
        (plant1 == 'cucumber' || plant1 == 'onions')) || (plant1 = 'onions' && (plant2 = 'carrots' ||
        plant2 = 'tomatoes')) || (plant2 = 'onions' && (plant1 = 'carrots' || plant1 = 'tomatoes')) 
         ) { return true;}

         else {return false;}

}